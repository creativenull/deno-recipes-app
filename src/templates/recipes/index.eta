<% layout('./layouts/base') %>

<div class="container">
    <ul>
        <% for (const recipe of it.recipes) { %>
            <li>
                <%= recipe.name %> - 
                    <a href="/recipes/<%= recipe.uuid %>">Open</a> | 
                    <a href="/recipes/edit/<%= recipe.uuid %>">Edit</a>
            </li>
        <% } %>
    </ul>
</div>
